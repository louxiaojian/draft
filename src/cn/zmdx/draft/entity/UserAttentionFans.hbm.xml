<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.draft.entity.UserAttentionFans" table="user_attention_fans" dynamic-insert="true">
	  <id name="id" column="id" type="int" >
			<generator class="native" />
		</id>
		<property name="attentionUserId" column="attention_user_id"/>
		<property name="fansUserId" column="fans_user_id"/>
		<property name="attentionTime" column="attention_time"/>
	</class>
	
</hibernate-mapping>
  