<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.draft.entity.Cycle" table="theme_cycle" dynamic-insert="true">
	  <id name="id" column="id" type="int" >
			<generator class="native" />
		</id>
		<property name="themeTitle" column="theme_title"/>
		<property name="starttime" column="starttime"/>
		<property name="endtime" column="endtime" />	
		<property name="status" column="status" />	
		<property name="bgUrl" column="bg_url"/>
		<property name="descs" column="descs" />
		<property name="tag" column="tag_url" />
		<property name="detailImageUrl" column="detail_image_url" />
		<property name="insideDetailImageUrl" column="inside_detail_image_url" />
		<property name="webDetailUrl" column="web_detail_url" />
		<property name="voteStartTime" column="vote_start_time" />
		<property name="voteEndTime" column="vote_end_time" />
		<property name="webTitleUrl" column="web_title_url" />
	</class>
	
</hibernate-mapping>
  