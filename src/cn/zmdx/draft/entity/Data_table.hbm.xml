<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.draft.entity.Data_table" table="data_table">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="content" column="content" />
		<property name="top" column="top" />
		<property name="step" column="step" />
		<property name="collect_time" column="collect_time" />
		<property name="release_time" column="release_time" />
		<property name="collect_website" column="collect_website" />
		<property name="title" column="title" />
		<property name="data_type" column="data_type" />
	</class>

</hibernate-mapping>
  