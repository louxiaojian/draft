<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="cn.zmdx.locker.entity.Data_img_table" table="data_img_table">
		<id name="id" column="id" type="int">
			<generator class="native" />
		</id>
		<property name="title" column="title" />
		<property name="url" column="url" />
		<property name="imgUrl" column="imgUrl" />
		<property name="release_time" column="release_time" />
		<property name="collect_time" column="collect_time" />
		<property name="top" column="top" />
		<property name="step" column="step" />
		<property name="collect_website" column="collect_website" />
		<property name="data_type" column="data_type" />
		<property name="data_sub" column="data_sub" />
		<property name="type" column="type" />
		<property name="userid" column="userid" />
		<property name="views" column="views" />
		<property name="data_view" column="data_view" />
		<property name="stick" column="stick" />
	</class>

</hibernate-mapping>
  